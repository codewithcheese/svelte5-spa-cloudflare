    "ai": "^3.1.1",
    "bits-ui": "^0.21.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "kysely": "^0.27.3",
    "lucide-svelte": "^0.378.0",
    "sqlocal": "^0.8.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.6"
  }
}
